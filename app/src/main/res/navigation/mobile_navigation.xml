    <fragment
        android:id="@+id/nav_inmueble"
        android:name="com.ulp.appinmobiliaria.ui.inmueble.InmuebleFragment"
        android:label="@string/menu_inmuebles"
        tools:layout="@layout/fragment_inmueble" />

    <fragment
        android:id="@+id/nav_inquilino"
        android:name="com.ulp.appinmobiliaria.ui.inquilino.InquilinoFragment"
        android:label="@string/menu_inquilinos"
        tools:layout="@layout/fragment_inquilino" />

    <fragment
        android:id="@+id/nav_contrato"
        android:name="com.ulp.appinmobiliaria.ui.contrato.ContratoFragment"
        android:label="@string/menu_contratos"
        tools:layout="@layout/fragment_contrato" />

    <activity
        android:id="@+id/loginActivity"
        android:name="com.ulp.appinmobiliaria.ui.login.LoginActivity"
        android:label="activity_login"
        tools:layout="@layout/activity_login" />

